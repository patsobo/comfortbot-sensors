{
	"name": "ljswitchboard-splash_screen",
	"version": "0.0.2",
	"description": "The splash screen & primary loading file for LabJack's Kipling program.",
	"author": {
		"name": "Chris Johnson",
		"url": "http://labjack.com"
	},
	"homepage": "https://github.com/chrisJohn404/ljswitchboard-splash_screen",
	"repository": {
		"type": "git",
		"url": "https://github.com/chrisJohn404/ljswitchboard-splash_screen"
	},
	"keywords": [
		"LabJack",
		"LJM",
		"T7",
		"ljswitchboard"
	],
	"license": {
		"type": "MIT",
		"url": "https://github.com/chrisJohn404/ljswitchboard-splash_screen/master/LICENSE"
	},
	"bugs": {
		"url": "https://github.com/chrisJohn404/ljswitchboard-splash_screen/issues"
	},
	"main": "lib/index.html",
	"node-main": "lib/main.js",
	"window": {
		"title": "Kipling",
		"icon": "lib/Kipling_128x128.png",
		"toolbar": false,
		"frame": false,
		"width": 480,
		"height": 300,
		"min_width":480,
		"min_height":300,
		"show": true,
		"resizable": false
	},
	"nodejs": true,
	"dependencies": {
		"q": "~1",
		"ljswitchboard-package_loader": "0.0.x",
		"ljswitchboard-require": "0.0.x",
		"ljswitchboard-ljm_driver_checker": "0.0.x",
		"ljswitchboard-window_manager": "0.0.x",
		"rimraf": "2.2.x",
		"fs.extra": "1.3.x",
	    "fs-extra": "0.18.x",

	    "async": "~0",
	    "yauzl": "2.3.1",
    	"concat-stream": "^1.4.6"
	},
	"un-managed-dependencies": {
		"semver": "4.2.0",
		"unzip": "0.1.11",
		"nw": "0.12.3",
		"nodewebkit": "0.11.6",
	    "ljswitchboard-core": "0.0.x"
	},
	"old-dependencies": {
		"async": "~0",
		"fs.extra": "1.3.x",
		"handlebars": "2.0.0",
		"nodeunit": "~0.9.0",
	    "rewire": "2.1.4",
	    "fs-extra": "0.18.x"
	},
  	"devDependencies": {
  		"nw": "0.12.3"
  	},
	"script_notes": "to run a script w/o using npm test, use npm run [script name]",
	"scripts": {
		"start": "npm run nw",
		"nodewebkit": "nodewebkit",
		"nw": "nw",
	    "test": "node ./node_modules/nodeunit/bin/nodeunit ./test/test.js"
	},
	"enableStartupConfig":false,
	"buildType": "release",
	"test": false,
	"testPersistentDataFolderName": "K3_DEV",
	"performLiveTests": false,
	"webkit": {
		"plugin": true
	},
	"js-flags": "--expose-gc",
	"persistentDataFolderName": "K3",
	"persistentDataVersion": "0.0.1",
	"forceRefreshOfPersistentData": false
}
